{
    "name": "chj/laravel",
    "description": "chj-laravel-demo",
    "keywords": ["chj-laravel"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "chenhuojia",
            "email": "1126089253@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "aws/aws-sdk-php": "^3.54",
        "barryvdh/laravel-debugbar": "^3.1",
        "caffeinated/modules": "v4.3.2",
        "chj/tree": "^1.0",
        "dingo/api": "2.0.0-alpha1",
        "fideloper/proxy": "~3.3",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "pda/pheanstalk": "^3.1",
        "predis/predis": "^1.1",
        "zhuzhichao/ip-location-zh": "^2.4"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "common/function.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
